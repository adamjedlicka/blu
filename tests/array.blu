var arr = [1, 2, "3", false, [5, 6]]

assert len(arr) == 5

assert arr[0] == 1
assert arr[2] == "3"
assert arr[3] == false
assert arr[4][0] == 5
assert arr[4][1] == 6

arr[1] = "Hello, World!"

assert arr[1] == "Hello, World!"

arr = []

assert len(arr) == 0

arr += 1

assert len(arr) == 1

assert arr[0] == 1

var arr1 = [1, 2, 3]
var arr2 = [4, 5, 6]
var arr3 = arr1 + arr2

assert len(arr3) == len(arr1) + len(arr2)

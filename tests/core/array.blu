assert [1, 2, 3, 4, 5].at(0) == 1
assert [1, 2, 3, 4, 5].at(2) == 3
assert [1, 2, 3, 4, 5].at(4) == 5

assert ["adf", "fgh", "jkl"].at(1).at(1) == "g"
assert ["adf", "fgh", "jkl"][1][1] == "g"

assert [1, 2, 3, 4, 5].slice(1, 2).equals([2, 3])
assert [1, 2, 3, 4, 5].slice(0, 0).equals([])
assert [1, 2, 3, 4, 5].slice(4, 1).equals([5])


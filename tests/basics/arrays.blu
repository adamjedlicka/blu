var arr1 = [1, 2, 3]
assert 2 == arr1[1]

arr1[] = 4
assert 4 == arr1[3]

arr1[0] = 5
assert 5 == arr1[0]

var arr2 = [false, true, nil, "string", 42, 3.14, ["another", "array"]]

assert arr2[0] == false
assert arr2[1] == true
assert arr2[2] == nil

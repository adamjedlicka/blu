var cnt = 0

for var i = 0; i < 10; i = i + 1 : cnt += i

assert cnt == 45

var a = 0
var b = 0

for var i = 0; i < 10; i += 1 {
    a += 1

    if i > 0 : break

    for var j = 0; j < 10; j += 1 {
        b += 1

        if j > 0 : break

        assert j == 0
    }

    assert i == 0
}

assert a == 2
assert b == 2

for var i = 0; i < 10; i += 1 {
    var tmp = 0

    for var j = 0; j < 10; j += 1 {
        tmp += j
    }

    assert 45 == tmp
}

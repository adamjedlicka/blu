class MyClass {}

assert MyClass().toString() == "MyClass"

assert 42.toString() == "42"
assert 3.1415.toString() == "3.1415"
assert nil.toString() == "nil"
assert true.toString() == "true"
assert false.toString() == "false"

assert false.isFalsey()
assert !true.isFalsey()
assert nil.isFalsey()
assert 0.isFalsey()
assert 1.isTruthy()

assert 420.getClass() == Number
assert true.getClass() == Boolean
assert false.getClass() == Boolean
assert nil.getClass() == Nil
assert "".getClass() == String
assert "Hello, World!".getClass() == String
assert [].getClass() == Array
assert [1, 2, 3, 4].getClass() == Array
assert [0, 1, 2, 3, 4].getClass() == Array
assert MyClass().getClass() == MyClass
assert 3.1.floor() == 3
assert 3.1.ceil() == 4

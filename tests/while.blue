var cnt = 0

var i = 0

while i < 10 {
    var j = 0

    while j < i + 1 {
        cnt = cnt + 1

        j = j + 1
    }

    assert j == i + 1

    i = i + 1
}

assert cnt == 55

cnt = 0

while cnt < 10 : cnt = cnt + 1

assert cnt == 10

i = 0

while i < 15 {
    if i == 10 : break

    var j = 0

    while j < 15 {
        if j == 5 : break

        j = j + 1
    }

    assert j == 5

    i = i + 1
}

assert i == 10
